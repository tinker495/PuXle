(define (problem rv-S-04)
  (:domain rovers)
  (:objects r1 - rover
            w1 w2 w3 w4 w5 - waypoint
            i1 - instrument
            s1 s2 s3 - sample
            o1 o2 o3 - objective)
  (:init
    (at r1 w1)
    (adj w1 w2)
    (adj w2 w1)
    (adj w2 w3)
    (adj w3 w2)
    (adj w3 w4)
    (adj w4 w3)
    (adj w4 w5)
    (adj w5 w4)
    (have-instrument r1 i1)
    (at-sample s1 w2)
    (at-sample s2 w4)
    (at-sample s3 w5)
    (visible w2 o1)
    (visible w4 o2)
    (visible w5 o3)
  )
  (:goal (and (communicated-image o1)
              (communicated-sample s1)
              (communicated-image o2)
              (communicated-sample s2)
              (communicated-image o3)))
)
